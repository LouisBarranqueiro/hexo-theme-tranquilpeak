{
  "main": {
    "files": [{
      "src": ["**"],
      "dest": "bin/to/",
      "expand": true,
      "cwd": "bin/from/"
    }],
    "verbose": true,
    "pretend": true
  },
  "rename": {
    "src": "bin/somefile.txt",
    "dest": "bin/somefile2.txt"
  },
  "windows": {
    "files": [{
      "src": ["www\\**\\*.*", "!**\\*.ts"],
      "dest": "cordova\\"
    }],
    "verbose": true,
    "pretend": false,
    "updateAndDelete": true
  },
  "no26": {
    "files": [{
      "cwd": "bin/p2/icons",
      "expand": true,
      "src": "**/*",
      "dest": "bin/p2/build/imgs/icons"
    }, {
      "cwd": "bin/p2/src/js",
      "expand": true,
      "src": "**/*",
      "dest": "bin/p2/build/js"
    }, {
      "expand": true,
      "src": "bin/p2/index.html",
      "dest": "bin/p2/build"
    }],
    "verbose": true,
    "updateAndDelete": true
  }
}
